{
  "name": "autofish-store-mobile",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:mobile": "tsc -b && vite build && cap sync",
    "generate-icons": "node generate-icons.cjs",
    "build-apk": "npm run generate-icons && npm run build && cap sync && cd android && gradlew.bat assembleDebug && cd ..",
    "build-apk-full": "node -e \"require('child_process').execSync(process.platform === 'win32' ? 'build-apk.bat' : './build-apk.sh', {stdio: 'inherit'})\"",
    "build-aab": "npm run generate-icons && npm run build && cap sync && cd android && gradlew.bat bundleRelease && cd ..",
    "build-aab-full": "node -e \"require('child_process').execSync(process.platform === 'win32' ? 'build-aab.bat' : './build-aab.sh', {stdio: 'inherit'})\"",
    "android": "cap open android",
    "ios": "cap open ios",
    "sync": "cap sync"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.2",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^6.2.1",
    "@capacitor/ios": "^6.2.1",
    "@ionic/react": "^8.6.2",
    "@ionic/react-router": "^8.6.2",
    "@types/lodash": "^4.17.16",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "crypto-js": "^4.2.0",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-toastify": "^11.0.5",
    "react-webcam": "^7.2.0",
    "styled-components": "^6.1.17",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.15.17",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "sharp": "^0.33.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
